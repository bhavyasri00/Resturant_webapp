{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "Frontend/dist",
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url"
  },
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "Backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/dist/index.html"
    }
  ]
}
